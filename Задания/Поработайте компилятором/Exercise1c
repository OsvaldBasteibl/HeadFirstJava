public class Exercise1c {
    public static void main(String[] args) {//добавил, так как без метода main невозмножно запустить программу
        int x = 5;
        while (x > 1){
            x = x - 1;
            if (x < 3){
                System.out.println("маленький икс");
            }
        }
    }
}
